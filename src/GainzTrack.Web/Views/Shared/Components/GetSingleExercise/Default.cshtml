@using GainzTrack.Web.Extensions
@model Exercise
@{
    Layout = null;
}

@{var id = Guid.NewGuid().ToString();}

<div id="onPost" class="tab">
    <input asp-for="ExerciseName" type="text" name="ExerciseName" readonly/>
    <input id="tab-@id" type="checkbox" name="tabs">
    <label for="tab-@id">@Model.ExerciseName</label>
    <span id="exercise-@id" class="remove-exercise-button">Remove</span>
    <div class="tab-content">
        <iframe src="@Model.VideoUrl.DecodeUrl()" frameborder="0"></iframe>
    </div>
    <hr />
</div>


