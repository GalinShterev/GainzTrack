@model GainzTrack.Web.ViewModels.UsersViewModels.GetAllUsersViewModel
<div class="dashboard-content-fluid">
    @foreach (var user in Model.Users)
    {
        <div class="card" style="box-shadow: 5px 5px 50px #888888;margin-left:10px;margin-top:10px;">
            <div class="ds-top"></div>
            <div class="avatar-holder">
                <img src="http://pluspng.com/img-png/fitness-hd-png-fitness-point-workout-exercise-journal-personal-trainer-body-tracker-3111-1024.png" alt="Default profile">
            </div>
            <div class="name">
                <a href="#" target="_blank">@user.Username</a>
                <p>@user.Title</p>
            </div>
            <div class="stats-container">
                <div class="stats-item">
                    <h5>Workouts</h5>
                    <p>@user.WorkoutsCreated</p>
                </div>
                <div class="stats-item">
                    <h5>Achievements</h5>
                    <p>@user.Achievements</p>
                </div>
                <div class="stats-item">
                    <h5>Points</h5>
                    <p>@user.AchievementPoints</p>
                </div>
            </div>
        </div>
    }
</div>